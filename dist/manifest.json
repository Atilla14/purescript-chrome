{
  "manifest_version": 2,
  "name": "Chromux",
  "description": "Tmux for chrome",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Tile your windows"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "history"
  ]
}
